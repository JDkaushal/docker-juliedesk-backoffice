.calendar-container {
    #calendar {

      .calendar-datepicker-sprite {
        background-image: url(<%= asset_path 'date_picker_blue.png' %>);
        background-size: 18px 22px;
        background-repeat: no-repeat;
        width: 18px;
        height: 22px;
        position: relative;
        left: -3px;
        top: -1px;
        display: inline-block;
      }

      .fc-header-title {
        cursor: pointer;
        padding: 8px !important;
        height: 33px;

        h2 {
          display: inline-block;
          font-weight: bold;
          color: $blue;
          position: relative;
          top: -6px;
        }

        &:hover {
          background-color: #F0F0F0;
          text-decoration: underline;
        }
      }

        .fc-icon-left-single-arrow:after {
            /*content: "\02039";*/
            content: url("<%= asset_path "calendar/calendar_past_icon.png" %>");
            font-weight: bold;
            color: #a0a0a0;
            zoom: 0.5;
            opacity: 0.6;
            padding: 0px;
            margin: 0px;
        }

        .fc-icon-right-single-arrow:after {
            /*content: "\0203A";*/
            content: url("<%= asset_path "calendar/calendar_future_icon.png" %>");
            font-weight: bold;
            color: #a0a0a0;
            zoom: 0.5;
            opacity: 0.6;
            padding: 0px;
            margin: 0px;
        }

        .fc-icon-left-double-arrow:after {
            /*content: "\02039";*/
            content: url("<%= asset_path "calendar/calendar_past_d_icon.png" %>");
            font-weight: bold;
            color: #a0a0a0;
            zoom: 0.5;
            opacity: 0.6;
            padding: 0px;
            margin: 0px;
        }

        .fc-icon-right-double-arrow:after {
            /*content: "\0203A";*/
            content: url("<%= asset_path "calendar/calendar_future_d_icon.png" %>");
            font-weight: bold;
            color: #a0a0a0;
            zoom: 0.5;
            opacity: 0.6;
            padding: 0px;
            margin: 0px;
        }

        .allday-mask {
            background-color: #FD9797;
            opacity: 0.5;
            position: absolute;
            z-index: 2;
            pointer-events: none;
        }


        .fc-event {
            // This min height prevents events shorter than 30 min to display correctly
            //min-height: 30px !important;

            &.suggestion-from-ai {
              background-color: $aiControlColor !important;
              overflow: visible !important;

              &:hover {
                background-color: $aiControlHighlightedColor !important;

                .call-to-actions-wrapper {
                  background-color: $aiControlHighlightedColor;
                }
              }

              &.highlighted {
                background-color: $aiControlHighlightedColor !important;

                .call-to-actions-wrapper {
                  background-color: $aiControlHighlightedColor;
                }
              }

              &:hover {
                z-index:999;
                /*min-width: 227px;*/
                /*min-height: 45px;*/

                /*.call-to-actions-wrapper {*/
                  /*display: block;*/
                /*}*/
              }
              .fc-event-inner {
                background-color: inherit;
                overflow: visible;
              }

              .call-to-actions-wrapper {
                //display: none;
                position: absolute;
                padding-top: 5px;
                right: 0px;
                top: 0px;
                min-height: 35px;
                width:65px;
                background-color: $aiControlColor;

                .call-to-action {
                  width: 25px;
                  display: inline-block;
                  height: 25px;
                  position: relative;
                  top: -1px;
                  left: 10px;

                  &.accept {
                    @include ai-date-suggestion-sprite-accept-circled();
                  }

                  &.reject {
                    @include ai-date-suggestion-sprite-reject-circled();
                  }
                }
              }

              .fc-event-inner {
                
                &.juliedesk-located-event {
                  border: none !important;
                }
              }
            }

            &.meeting-room-unavailable {
                pointer-events: none;
                z-index: 0 !important;
                background-color: #E37F7F !important;

                &:hover {
                    z-index: 999 !important;
                }

                .fc-event-inner {

                    &.juliedesk-located-event {
                        border: none !important;
                    }

                    .fc-event-title {

                        .sprite {
                            background-image: url(<%= asset_path 'meeting_room.png' %>);
                            background-size: 15px 14px;
                            background-repeat: no-repeat;
                            width: 15px;
                            height: 14px;
                            position: relative;
                            left: -3px;
                            top: 0px;
                            display: inline-block;
                        }

                        .text {
                            margin-left: 5px;
                            font-style:italic;
                            font-weight: 400;
                        }
                    }
                }


            }

            .travel-time-wrapper {
                height: 100%;
                width: 100%;
                text-align: right;
                vertical-align: bottom;

                &.before {

                }

                &.after {

                }

                .travel-time-inner-wrapper {

                    .duration {
                        position: relative;
                        left: -6px;
                    }

                    .sprite {
                        background-image: url(<%= asset_path 'transport.png' %>);
                        background-size: 13px 16px;
                        background-repeat: no-repeat;
                        width: 13px;
                        height: 16px;
                        position: relative;
                        left: -3px;
                        top: 3px;
                        display: inline-block;
                    }
                }
            }

            .default-delay-wrapper {
                height: 100%;

                .default-delay-inner-wrapper {
                    text-align: center;

                    display: table;
                    width: 100%;
                    height: 100%;

                    .duration {
                        font-size: 11px;
                        font-weight: bold;

                        display: table-cell;
                        vertical-align: middle;
                    }

                    .sprite {
                        background-image: url(<%= asset_path 'delay.png' %>);
                        background-size: 14px 14px;
                        background-repeat: no-repeat;
                        width: 13px;
                        height: 13px;
                        position: relative;
                        left: -82px;
                        top: 2px;
                        display: inline-block;
                    }
                }
            }

            .fc-event-location {
                margin-left: 5px;
                .location {
                    width: 90%;
                    display: inline-block;
                    // Prevent multi-lignes
                    white-space: nowrap;
                    overflow: hidden;
                    font-size: 11px;
                    font-weight: 200;
                }

                .sprite {
                    background-image: url(<%= asset_path 'location.png' %>);
                    background-size: 9px 15px;
                    background-repeat: no-repeat;
                    width: 9px;
                    height: 15px;
                    position: relative;
                    left: -3px;
                    display: inline-block;
                }
            }

            &.not-available {
                z-index: 1;
            }

            &.event-allday {

                .allday-busy-img {
                    cursor: pointer;
                    position: absolute;
                    top: 2px;
                    left: 4px;
                    background-size: 15px 22px;
                    background-repeat: no-repeat;
                    width: 20px;
                    height: 22px;

                    &.busy {
                        background-image: url(<%= asset_path 'lock_busy.png' %>) !important;
                    }
                    &.free {
                        background-image: url(<%= asset_path 'lock_free.png' %>) !important;
                    }
                }

                .fc-event-title {
                    margin-left: 20px;
                }
            }
        }
        a.fc-event {
            color: #fff;

            &:hover, &:focus, &:visited {
                color: #fff;
            }
        }

        th {
            font-weight: normal;
        }

        table {
            border-collapse: collapse;
        }

        .fc-event-inner {

            .delete-event-button {
                height: 14px;
                width: 14px;
                position: absolute;
                right: 4px;
                top: 4px;
                opacity: 0.6;
                content: url("<%= asset_path "calendar/delete_calendar.png" %>");
            }

            &.juliedesk-selected-event {
                background: rgba(40, 166, 203, 0.9) !important;
            }
            &.juliedesk-located-event {
                border-left-width: 8px !important;
                border-left-color: #444 !important;
                line-height: 15px;
            }

            &:hover {
                .delete-event-button {
                    opacity: 1.0;
                }
            }
        }

        .fc-widget-content.fc-today {
            background: #E7F2F5;
        }
        .fc-widget-content.fc-past {
            background: #eee;
        }
        .fc-button-today {
            padding: 3px 10px;
            font-size: 16px;
            color: rgb(244, 65, 109);
            opacity: 0.9;

            &:hover {
                opacity: 1.0;
            }

            &.fc-state-disabled {
                color: #ccc;
                opacity: 1.0;
            }
        }

        .fc-header {
            margin: 0 0 10px 0;
            position: relative;
            background: #fff;
            z-index: 9;

            &.as-header {
                border-bottom: 1px solid #ccc;
            }

            .fc-header-title {
                padding: 11px 0;
                h2 {
                    font-weight: 200;
                    font-size: 16px;
                }
            }
        }

        .fc-widget-header.today {
            font-weight: 600;
            color: rgba(40, 166, 203, 0.9);
        }
        .events-count-label {
            position: absolute;
            top: 3px;
            left: 8px;
            height: 22px;
            width: 22px;
            font-size: 12px;
            text-align: center;
            border-radius: 12px;
            line-height: 20px;
            font-weight: 400;

            border: 1px solid #aaa;
            background: #fff;
            color: #aaa;

            &.ras {
                background: rgba(40, 166, 203, 0.9);
                border-color: rgba(40, 166, 203, 0.9);
                color: #fff;
            }

            &.best-of-worst {
                background: #aaa;
                border-color: #aaa;
                color: #fff;
            }
        }
        #calendars-list-popup-toggle-button {
            height: 26px;
            width: 26px;
            margin-bottom: 5px;
            cursor: pointer;
            content: url("<%= asset_path "calendar/settings_icon.png" %>");

            &:hover {
                content: url("<%= asset_path "calendar/settings_icon_hover.png" %>");
            }
        }
    }
}