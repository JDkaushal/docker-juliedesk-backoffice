<script>
    $(function() {
        window.activateCalendarWithParams({
            mode: "free_calendar",
            date_times: [],
            email: "<%= @message.messages_thread.account_email %>",
            duration: <%= @message.messages_thread.computed_data[:duration] || 60 %>
        });

        $(".calendar-container").addClass("minimized");

        window.setReplyMessage("", "all")
    });
</script>